{
  "name": "userApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "postversion": "cd android && ./gradlew assembleStagingRelease",
    "android:staging": "npx react-native run-android --variant=stagingdebug --appId=com.userapp.test",
    "android:staging-release": "npx react-native run-android --variant=stagingrelease  --appId=com.userapp.test",
    "android:dev": "npx react-native run-android --port=8081 --variant=developmentdebug --appId=com.userapp",
    "android:dev-release": "npx react-native run-android --variant=developmentrelease --appId=com.userapp",
    "android:qa": "npx react-native run-android --variant=qadebug --appId=com.userapp",
    "android:qa-release": "npx react-native run-android --variant=qarelease --appId=com.userapp",
    "android:prod": "npx react-native run-android --variant=productiondebug --appId=com.userapp",
    "android:prod-release": "npx react-native run-android --variant=productionrelease --appId=com.userapp"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.18.1",
    "@stripe/stripe-react-native": "^0.27.2",
    "axios": "^1.4.0",
    "i18n-js": "^4.2.3",
    "react": "18.2.0",
    "react-native": "0.71.8",
    "react-native-config": "^1.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.73.9",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "jest": {
    "preset": "react-native"
  }
}
